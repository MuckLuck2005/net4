 using System;

class A
{
    protected int a;
    protected int b;

    public A(int a, int b)
    {
        this.a = a;
        this.b = b;
    }
}

class B : A
{
    private int d;

    public B(int a, int b, int d) : base(a, b)
    {
        this.d = d;
    }

    public int C2
    {
        get
        {
            int result = a * b * d;
            if (result > 100)
                return result;
            else
                return result * 2;
        }
    }
}

class Program
{
    static void Main(string[] args)
    {
        A objA = new A(2, 3);

        B objB1 = new B(2, 3, 4);

        B objB2 = new B(5, 6, 7);

        if (objB1 is B)
            Console.WriteLine("Значення властивості C2 для об'єкта класу B (спадкоємця класу A): " + objB1.C2);
        if (objB2 is B)
            Console.WriteLine("Значення властивості C2 для другого об'єкта класу B: " + objB2.C2);

        Console.ReadLine();
    }
}
